@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager


@if (SignInManager.IsSignedIn(User))
{

            <a asp-area="Identity" asp-page="/Account/Manage/Index" title="Manage"> @User.Identity?.Name</a>


    <form class="inlinedispform" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
        <a href="javascript:void(0);" onclick="document.forms[0].submit();">Logout</a>
        </form>
  
}
else
{
    
        <a  asp-area="Identity" asp-page="/Account/Register">Register</a>
   
   
        <a  asp-area="Identity" asp-page="/Account/Login">Login</a>

}

